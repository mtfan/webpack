{
  "name": "vue-webpack",
  "version": "1.0.0",
  "description": "vue-base",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.production.config.js --progress --colors",
    "build:analy": "cross-env NODE_ENV=production ANALY=true webpack --config ./webpack.production.config.js --progress --colors",
    "build:log": "cross-env NODE_ENV=development webpack --config ./webpack.production.config.js --progress --colors",
    "build:log:analy": "cross-env NODE_ENV=development ANALY=true webpack --config ./webpack.production.config.js --progress --colors",
    "dev:dev": "cross-env NODE_ENV=development webpack-dev-server --progress --colors --watch --open",
    "dev:mock": "nodemon mock/app.js",
    "dev": "npm-run-all --parallel dev:**",
    "eslint": "eslint ./src --ext .js,.vue",
    "fix": "eslint --fix ./src --ext .js,.vue"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run fix && npm run eslint"
    }
  },
  "author": "hulei",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.17.1",
    "fastclick": "^1.0.6",
    "jsonp": "^0.2.1",
    "mint-ui": "^2.2.13",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
